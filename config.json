      "BYR",
      "TJU",
      "PTER",
      "MTeam"
    ],
    "KeepTorrentTime": 24,
    //种子多久时间内不会被删除,自添加开始计算,默认0,单位:小时

    "CheckTrackerHTTPS": true
    //是否自动更新种子的tracker为https
